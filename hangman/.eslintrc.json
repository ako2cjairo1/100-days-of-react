{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended", "airbnb"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2018
	},
	"plugins": ["react"],
	"rules": {
		"indent": ["error", 2],
		"quotes": ["error", "double"],
		"semi": ["error", "always"],
		"no-console": ["warn", { "allow": ["info", "error"] }],
		"react/prop-types": 0,
		"react/jsx-filename-extension": 0
	}
}
